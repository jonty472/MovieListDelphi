unit MovieWatchlist;

interface

uses
  System.Generics.Collections,
  Movie;

type
  Watchlist = class
    private

    _movieWatchlist: TObjectList<MovieDetails>;
    _lastSort: string;


    public

    function SortLowestRating(movieItem: MovieDetails): integer;
    function SortHighestRating(movieItem: MovieDetails): integer;

  end;
implementation

end.
